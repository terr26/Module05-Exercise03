<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Module07Data_Access.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Module07Data_Access"
    Shell.FlyoutBehavior="Disabled"
    xmlns:views="clr-namespace:Module07Data_Access.View"
    Title="Module07Data_Access">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Employee List"
        ContentTemplate="{DataTemplate views:ViewPersonal}"
        Route="ViewPersonal" />
</Shell>
